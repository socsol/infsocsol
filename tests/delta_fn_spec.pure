namespace delta_fn_spec;

delta_fn u _ _        = u;

<!!: 
"delta_fn" => [delta_fn a b c should be a
               | a,b,c = 10 from (double,double,double) ]
!>;

let octave_delta_fn   = octave_func ("pure_call('delta_fn_spec::delta_fn', u, x, t)", "u", "x", "t");

<!!: 
"octave_delta_fn" => [octave_call octave_delta_fn 1 (a, b, c) should be delta_fn a b c
                      | a,b,c = 10 from (double,double,double) ]
!>;
